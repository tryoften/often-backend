        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "build",
        "moduleResolution": "node",
				"target": "es5"
    },
    "exclude": [
        "node_modules"
    ]
}
